	<update>
		<name>Joomla! {to} Nightly Build</name>
		<description>Joomla! CMS</description>
		<element>joomla</element>
		<type>file</type>
		<version>{tag}</version>
		<infourl title="Joomla! Nightly Builds">https://developer.joomla.org/nightly-builds.html</infourl>
		<downloads>
			<downloadurl type="full" format="zip">https://developer.joomla.org/nightlies/Joomla_{tag}-Development-Update_Package.zip</downloadurl>
		</downloads>
		<tags>
			<tag>stable</tag>
		</tags>
		<supported_databases mysql="{mysql}" mariadb="{mariadb}" postgresql="{pgsql}" />
		<php_minimum>{php}</php_minimum>
		<maintainer>Joomla! Production Department</maintainer>
		<maintainerurl>https://www.joomla.org</maintainerurl>
		<targetplatform name="joomla" version="{from}" />
	</update>

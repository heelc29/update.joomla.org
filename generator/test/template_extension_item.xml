	<update>
		<name>Joomla! {to}</name>
		<description>Joomla! {to} CMS</description>
		<element>joomla</element>
		<type>file</type>
		<version>{tag}</version>
		<infourl title="Joomla {tag} Release">https://github.com/joomla/joomla-cms/releases/tag/{tag}</infourl>
		<downloads>
			<downloadurl type="full" format="zip">https://github.com/joomla/joomla-cms/releases/download/{tag}/Joomla_{tag}-{stability_long}-Update_Package.zip</downloadurl>
		</downloads>
		<tags>
			<tag>{stability}</tag>
		</tags>
		<sha256>{sha256}</sha256>
		<sha384>{sha384}</sha384>
		<sha512>{sha512}</sha512>
		<supported_databases mysql="{mysql}" mariadb="{mariadb}" postgresql="{pgsql}" />
		<php_minimum>{php}</php_minimum>
		<maintainer>Joomla! Production Department</maintainer>
		<maintainerurl>https://www.joomla.org</maintainerurl>
		<targetplatform name="joomla" version="{from}" />
	</update>
